{
	"collection_configuration": {
		"columns": [
			{
				"column_name": "name",
				"column_description": "Nombre",
				"is_visible": true,
				"typeof": "string"
			},
			{
				"column_name": "specie",
				"column_description": "Especie",
				"is_visible": true,
				"typeof": "string"
			},
			{
				"column_name": "gender",
				"column_description": "Género",
				"is_visible": true,
				"typeof": "string"
			},
			{
				"column_name": "age",
				"column_description": "Edad",
				"is_visible": true,
				"typeof": "int"
			},
			{
				"column_name": "author",
				"column_description": "Autor / Productor",
				"is_visible": true,
				"typeof": "string"
			},
			{
				"column_name": "appear_in",
				"column_description": "Aparece En",
				"is_visible": true,
				"typeof": "string"
			},
			{
				"column_name": "appear_format",
				"column_description": "Formato en Donde Aparece",
				"is_visible": false,
				"typeof": "string"
			},
			{
				"column_name": "created_date",
				"column_description": "Fecha de Creación",
				"is_visible": false,
				"typeof": "date"
			}
		],
		"data": {
			"snippets": [
				{
					"column_name": "name",
					"method": "replace",
					"replace_with": "<i class='fa-solid fa-heart'></i> {value}",
					"column_related": ""
				},
				{
					"column_name": "gender",
					"method": "case",
					"replace_with": [
						{
							"case": "Masculino",
							"replace": "<i class='fa-solid fa-mars'></i> {value}"
						},
						{
							"case": "Femenino",
							"replace": "<i class='fa-solid fa-venus'></i> {value}"
						}
					],
					"column_related": ""
				},
				{
					"column_name": "appear_in",
					"method": "when",
					"replace_with": [
						{
							"case": "videogame",
							"replace": "<i class='fa-solid fa-gamepad'></i> {value}"
						},
						{
							"case": "comic",
							"replace": "<i class='fa-solid fa-book-open'></i> {value}"
						},
						{
							"case": "original",
							"replace": "<i class='fa-solid fa-paintbrush'></i> {value}"
						},
						{
							"case": "serie",
							"replace": "<i class='fa-solid fa-tv'></i> {value}"
						},
						{
							"case": "movie",
							"replace": "<i class='fa-solid fa-film'></i> {value}"
						}
					],
					"column_related": "appear_format"
				}
			],
			"conditions": [
				{
					"column_name": "age",
					"operator": "==",
					"value_to_compare": "0",
					"replace_with": "Desconocida"
				},
				{
					"column_name": "author",
					"operator": "==",
					"value_to_compare": "",
					"replace_with": "Desconocido"
				}
			]
		}
	},
