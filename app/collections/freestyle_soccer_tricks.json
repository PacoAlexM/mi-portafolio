{
	"collection_configuration": {
		"columns": [
			{
				"column_name": "trick",
				"column_description": "Truco",
				"is_visible": true,
				"typeof": "string"
			},
			{
				"column_name": "abbreviation",
				"column_description": "Abreviación",
				"is_visible": true,
				"typeof": "string"
			},
			{
				"column_name": "trick_type",
				"column_description": "Tipo de Truco",
				"is_visible": true,
				"typeof": "string"
			},
			{
				"column_name": "skill_level",
				"column_description": "Dificultad",
				"is_visible": true,
				"typeof": "int"
			},
			{
				"column_name": "can_i_perform_it",
				"column_description": "¿Puedo realizarlo?",
				"is_visible": true,
				"typeof": "bool"
			},
			{
				"column_name": "how_to_perform_it",
				"column_description": "Tutorial",
				"is_visible": true,
				"typeof": "string"
			},
			{
				"column_name": "where_to_see",
				"column_description": "plataforma",
				"is_visible": false,
				"typeof": "string"
			}
		],
		"data": {
			"snippets": [
				{
					"column_name": "trick",
					"method": "replace",
					"replace_with": "<i class='fa-regular fa-futbol'></i> {value}",
					"column_related": ""
				},
				{
					"column_name": "trick_type",
					"method": "case",
					"replace_with": [
						{
							"case": "Suelo",
							"replace": "<i class='fa-regular fa-window-minimize'></i> {value}"
						},
						{
							"case": "Inferior",
							"replace": "<i class='fa-solid fa-person-walking'></i> {value}"
						},
						{
							"case": "Medio",
							"replace": "<i class='fa-solid fa-person-running'></i> {value}"
						},
						{
							"case": "Superior",
							"replace": "<i class='fa-solid fa-person-falling'></i> {value}"
						}
					],
					"column_related": ""
				},
				{
					"column_name": "can_i_perform_it",
					"method": "case",
					"replace_with": [
						{
							"case": true,
							"replace": "<i class='fa-solid fa-face-laugh-beam'></i> Si"
						},
						{
							"case": false,
							"replace": "<i class='fa-solid fa-face-sad-cry'></i> No"
						}
					],
					"column_related": ""
				},
				{
					"column_name": "skill_level",
					"method": "for",
					"replace_with": "<i class='fa-solid fa-star'></i>",
					"column_related": ""
				}
			],
			"conditions": [
				{
					"column_name": "abbreviation",
					"operator": "==",
					"value_to_compare": "",
					"replace_with": "No Tiene"
				}
			]
		}
	},
